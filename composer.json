{
  "name": "remorhaz/php-json-parser",
  "description": "JSON (RFC 8259) parser written in PHP",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Edward Surov",
      "email": "zoohie@gmail.com",
      "role": "Developer"
    }
  ],
  "require": {
    "php": "^7.1",
    "remorhaz/php-unilex": "^0.0.11"
  },
  "require-dev": {
    "phpunit/phpunit": "^7.0 !=7.0.1",
    "phing/phing": "^2.16",
    "squizlabs/php_codesniffer": "^3.2.0"
  },
  "autoload": {
    "psr-4": {
      "Remorhaz\\JSON\\Parser\\": ["src/", "generated/"]
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Remorhaz\\JSON\\Parser\\Test\\": "tests/",
      "Remorhaz\\JSON\\Parser\\Example\\": "examples/src",
      "Remorhaz\\JSON\\Parser\\Example\\Test\\": "examples/tests"
    }
  }
}
